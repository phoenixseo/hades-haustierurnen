{
    "name": "phoenixseo/urneco-tierurnen",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Frank Pfabigan",
            "role": "SEO Consultant",
            "email": "frank.pfabigan@phoenixseo.de",
            "homepage": "https://phoenixseo.de"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.8.0",
        "drupal/core-composer-scaffold": "^8.8.0",
        "drush/drush": "^9.7.1 | ^10.0.0",
        "vlucas/phpdotenv": "^4.0",
        "webflo/drupal-finder": "^1.0.0",
        "zaporylie/composer-drupal-optimizations": "^1.0",
        "drupal/admin_toolbar": "^2",
        "drupal/better_exposed_filters": "^5",
        "drupal/field_group": "^3",
        "drupal/file_browser": "^1",
        "drupal/image_effects": "^3",
        "drupal/libraries": "^3",
        "drupal/crop": "^2",
        "drupal/embed": "^1",
        "drupal/better_normalizers": "^1",
        "drupal/dropzonejs": "^2",
        "drupal/entity_browser": "^2",
        "drupal/entity_embed": "^1",
        "drupal/entity_reference_revisions": "^1",
        "drupal/flippy": "^1",
        "drupal/fontawesome": "^2",
        "drupal/image_widget_crop": "^2",
        "drupal/inline_entity_form": "^1",
        "drupal/jquery_ui": "^1",
        "drupal/jquery_ui_datepicker": "^1",
        "drupal/jquery_ui_slider": "^1",
        "drupal/jquery_ui_touch_punch": "^1",
        "drupal/mailchimp": "^1",
        "drupal/onesignal": "^1",
        "drupal/paragraphs": "^1",
        "drupal/superfish": "^1",
        "drupal/video_embed_field": "^2",
        "drupal/views_infinite_scroll": "^1",
        "drupal/webform": "^5",
        "drupal/metatag": "^1",
        "drupal/pathauto": "^1",
        "drupal/redirect": "^1",
        "drupal/schema_metatag": "^1",
        "drupal/simple_sitemap": "^3",
        "drupal/token": "^1",
        "drupal/imageapi_optimize_resmushit": "^2",
        "drupal/blazy": "^2",
        "drupal/imageapi_optimize": "^4",
        "drupal/address": "^1",
        "drupal/search_api": "^1",
        "drupal/fontyourface": "^3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ]
        }
    },
    "homepage": "https://urneco-tierurnen.de"
}
